    <div class="col-md-4">
        <p>Przystanki</p>
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.Id)
            </dt>
            <dd>
                @Html.DisplayFor(model => model.Id)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.PlaceName)
            </dt>
            <dd>
                @Html.DisplayFor(model => model.PlaceName)
